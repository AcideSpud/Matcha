<% include header_dash.ejs %>

<div class="row">

    <% for (ret of ret) { %>
    <div class="col-sm-6 col-md-4">
        <div class="thumbnail" style="background-image: linear-gradient(90deg,rgb(127, 18, 47), rgb(200, 255, 255) );">
            <img src="<%= ret.img_path %>" alt="">
            <div class="caption" id="mybox">
                <h3><%=ret.nom%> <%= ret.prenom %></h3>
                <p id="test"><%= ret.bio %>...</p>

                <p><a href="#" class="btn btn-primary" role="button">Like</a> <a href="/profile/<%= ret.name %>" class="btn btn-default" role="button">View profil</a></p>

            </div>
        </div>
    </div>
    <% } %>
</div>





<% include footer %>
